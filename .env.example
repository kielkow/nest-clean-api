# Auth
JWT_SECRET='nestcleanapi'
JWT_EXPIRES_IN='1d'
JWT_PRIVATE_KEY=''
JWT_PUBLIC_KEY=''

# Server
PORT=3333
NODE_ENV=dev
HOST='0.0.0.0'

# Database
POSTGRESQL_DATABASE=nestcleanapi
POSTGRESQL_USERNAME=postgresql
POSTGRESQL_PASSWORD=postgresql
DATABASE_URL='postgresql://postgresql:postgresql@localhost:5432/nestcleanapi?schema=public' # local=localhost:5432 | docker=postgresql:5432

# Storage
STORAGE_TYPE=local
STORAGE_LOCAL_PATH='./uploads'
STORAGE_S3_BUCKET='nestcleanapi'
STORAGE_S3_REGION='us-east-1'
STORAGE_S3_ACCESS_KEY_ID='YOUR_ACCESS_KEY_ID'
STORAGE_S3_SECRET_ACCESS_KEY='YOUR_SECRET_ACCESS_KEY'
STORAGE_CLOUDFLARE_ID='YOUR_CLOUDFLARE_ID'
